{
  "name": "ux_backend",
  "version": "0.0.1",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "start": "cd database && npx prisma deploy && cd .. && node server.js",
    "dev-start-app": "npm run dev-prisma-deploy && node server.js",
    "dev-prisma-deploy": "npm run dev-generate-docs && cd database && prisma deploy -e ../.env && prisma generate -e ../.env",
    "dev-prisma-deploy-f": "npm run dev-generate-docs && cd database && prisma deploy -f -e ../.env && prisma generate -e ../.env",
    "dev-prisma-reset": "cd database && prisma reset -e ../.env",
    "dev-prisma-seed": "cd database && prisma seed -r -e ../.env",
    "dev-clear": "sudo rm -rf ./data ./uploads && docker-compose -f docker_compose_dev.yml rm -f",
    "dev-start": "docker-compose -f docker_compose_dev.yml up -d",
    "dev-stop": "docker-compose -f docker_compose_dev.yml stop",
    "dev-generate-docs": "cd ./docs && npx redoc-cli bundle swagger.yml && cd ..",
    "prod-build": "docker-compose build",
    "prod-up": "docker-compose up -d",
    "prod-start": "docker-compose start",
    "prod-full-restart": "npm run prod-stop && npm run prod-clear && npm run prod-build && npm run prod-up",
    "prod-clear": "sudo rm -rf ./data ./uploads && docker-compose rm -f",
    "prod-logs": "docker-compose logs -f",
    "prod-stop": "docker-compose stop",
    "test": "mocha"
  },
  "dependencies": {
    "agenda": "^2.0.2",
    "agendash": "^0.4.0",
    "argon2": "^0.19.3",
    "dataloader": "^1.4.0",
    "express": "^4.16.3",
    "express-rate-limit": "^3.2.0",
    "graphql-cost-analysis": "^1.0.2",
    "graphql-import": "^0.7.1",
    "graphql-shield": "^3.2.5",
    "graphql-yoga": "^1.16.2",
    "jsonwebtoken": "^8.3.0",
    "log4js": "^3.0.5",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "multer": "^1.4.1",
    "nodemailer": "^4.6.8",
    "prisma-client-lib": "^1.18.0",
    "pug": "^2.0.3",
    "validator": "^10.8.0"
  },
  "resolutions": {
    "graphql": "^14.0.2"
  },
  "devDependencies": {
    "prisma": "^1.18.1",
    "redoc-cli": "^0.6.4"
  }
}
