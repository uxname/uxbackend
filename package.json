{
  "name": "ux_backend",
  "version": "0.0.1",
  "main": "src/cluster.js",
  "author": "uxname",
  "license": "MIT",
  "scripts": {
    "dev-start-simple": "nodemon -e \"js json graphql\" node src/server.js",
    "dev-start-app": "npm run dev-prisma-deploy && node src/cluster.js",
    "dev-prisma-deploy": "npm run dev-generate-docs && cd src/database && prisma deploy -e ../../.env && prisma generate -e ../../.env",
    "dev-prisma-deploy-f": "npm run dev-generate-docs && cd src/database && prisma deploy -f -e ../../.env && prisma generate -e ../../.env",
    "dev-prisma-reset": "cd src/database && prisma reset -e ../../.env",
    "dev-prisma-seed": "cd src/database && prisma seed -r -e ../../.env",
    "dev-clear": "npm run clear && docker-compose -f docker-compose-dev.yml rm -f",
    "dev-start": "docker-compose -f docker-compose-dev.yml up -d",
    "dev-stop": "docker-compose -f docker-compose-dev.yml stop",
    "dev-generate-docs": "cd ./docs && npx redoc-cli bundle swagger.yml && cd ..",
    "clear": "sudo rm -rf ./data ./uploads ./logs ./config",
    "prod-build": "docker-compose build",
    "prod-init": "mkdir -p data/config && cp src/config/config.js data/config/config.js && cp .env_example .env",
    "prod-start-app": "cd src/database && npx prisma deploy && cd .. && node cluster.js",
    "prod-up": "docker-compose up -d",
    "prod-start": "docker-compose start",
    "prod-full-restart": "npm run prod-stop && npm run prod-clear && npm run prod-build && npm run prod-up",
    "prod-clear": "npm run clear && docker-compose rm -f",
    "prod-logs": "docker-compose logs -f",
    "prod-stop": "docker-compose stop",
    "test": "mocha src/test",
    "sonar": "npx sonarqube-scanner -D project.settings=sonar-scanner.properties",
    "sms": "node src/helper/SecureMemStorageCLI.js"
  },
  "dependencies": {
    "agenda": "^2.0.2",
    "agendash": "^0.4.0",
    "argon2": "^0.19.3",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "express-rate-limit": "^3.3.2",
    "graphql-cost-analysis": "^1.0.2",
    "graphql-import": "^0.7.1",
    "graphql-shield": "^4.1.0",
    "graphql-yoga": "^1.16.7",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "log4js": "^3.0.6",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "multer": "^1.4.1",
    "nodemailer": "^4.7.0",
    "pg": "^7.6.1",
    "prisma-client-lib": "^1.21.1",
    "pug": "^2.0.3",
    "rate-limit-redis": "^1.5.0",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "serve-index": "^1.9.1",
    "terminal-kit": "^1.26.5",
    "validator": "^10.9.0"
  },
  "resolutions": {
    "graphql": "^14.0.2"
  },
  "devDependencies": {
    "nodemon": "^1.18.7",
    "prisma": "^1.21.1",
    "redoc-cli": "^0.7.0",
    "sonarqube-scanner": "^2.1.2"
  }
}
